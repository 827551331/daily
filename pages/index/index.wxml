<!--index.wxml-->
<page-meta root-font-size="system" />
<view class="page" data-weui-theme="{{theme}}" data-weui-mode="{{mode}}">
  <view class="logo-view">
    <image class="logo" src="../../utils/image/md.jpeg" mode="heightFix"/>
  </view>
  <view class="">
    <view class="weui-form__control-area">
      <view class="weui-cells__group weui-cells__group_form">
        <view class="weui-cells">
          <view class="weui-cell weui-cell_active">
            <view class="weui-cell__hd"><label class="weui-label">密钥</label></view>
            <view class="weui-cell__bd">
              <input class="weui-input" placeholder="火速填写" placeholder-class="weui-input__placeholder" />
            </view>
          </view>
        </view>
      </view>
    </view>
    <view class="weui-form__tips-area">
      <view class="weui-form__tips">
        * 输入 6 位密钥登入
      </view>
    </view>
    <view class="weui-form__opr-area">
      <a aria-role="button" class="weui-btn weui-btn_primary" bindtap="navigateToEditor">确定</a>
    </view>
    <view class="weui-form__extra-area">
      <view class="weui-footer">
        <view class="weui-footer__links">
          <navigator url="javascript:" class="weui-footer__link">yibaby runner</navigator>
        </view>
        <view class="weui-footer__text">Copyright © 2008-2023 weui.io</view>
      </view>
    </view>
  </view>
</view>